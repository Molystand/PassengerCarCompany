<Window x:Class="PassengerCarCompany.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PassengerCarCompany"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="640"
        Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Button Name="btnBusTable" Content="Таблица Bus" Click="btnBusTable_Click"/>
        <Button Name="btnBusStopTable" Content="Таблица BusStop" Click="btnBusStopTable_Click"
                Grid.Row="1"/>
    </Grid>
</Window>
